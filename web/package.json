{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:handlers && npm run build:server && concurrently \"node run-api.js\" \"vite\"",
    "build:handlers": "tsc -p tsconfig.handlers.json",
    "build:server": "tsc -p tsconfig.server.json",
    "dev:api": "node run-api.js",
    "dev:vite": "vite",
    "dev:vercel": "npm run build:handlers && npm run build:server && concurrently \"node run-api.js\" \"vercel dev --listen 3000\"",
    "dev:ngrok": "ngrok http 3000",
    "build": "npm run build:handlers && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "vercel deploy --prod",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@prisma/client": "^6.9.0",
    "@vercel/node": "^3.0.0",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "openai": "^6.16.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "resend": "^4.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.25",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prisma": "^6.9.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^6.3.5"
  }
}
